programa {
  funcao inicio() {
 inteiro velocidade, conversao,tempo
 escreva ("Qual a velocidade do veículo em km/h? ")
 leia (velocidade)
 conversao= velocidade/3.6
 escreva ("A velocidade convertida em m/s é: ")
 escreva (conversao)
 escreva ("m/s \n")
 tempo= 434/velocidade
 escreva ("Para uma viagem de São Paulo até o Rio de Janeiro, o tempo levado para a chegada com base na velocidade média do veículo será aproximadamente: ")
 escreva (tempo)
 escreva (" horas")

    
  }
}
