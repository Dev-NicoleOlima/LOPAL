programa {
  funcao inicio() {
  inteiro volume,largura,profundidade,altura
  escreva("insira a Altura da caixa d'água em centímetros: " )  
  leia (altura)
  escreva ("insira a largura da caixa d'água centímetros: ")
  leia (largura)
  escreva ("insira a profundidade da caixa d'água centímetros: ")
  leia (profundidade)
  volume= altura*largura*profundidade
  escreva ("o volume da caixa da água é: ")
  escreva (volume)
  escreva ("cm")
  }
}
